# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Bonoloto
GET        /bonoloto/lastResult                  com.serinus.loto.controllers.BonolotoController.lastResult
# Cuponazo Once
GET        /cuponazoonce/lastResult              com.serinus.loto.controllers.CuponazoOnceController.lastResult
#Euromillones
GET        /euromillones/lastResult              com.serinus.loto.controllers.EuromillonesController.lastResult
GET        /euromillones/result/:date            com.serinus.loto.controllers.EuromillonesController.result(date)
# Gordo
GET        /gordo/lastResult                     com.serinus.loto.controllers.GordoController.lastResult
# Lottoturf
GET        /lottoturf/lastResult                 com.serinus.loto.controllers.LototurfController.lastResult
# Primitiva
GET        /primitiva/lastResult                 com.serinus.loto.controllers.PrimitivaController.lastResult

# Scraper [Get Data]
GET        /scraper/once                         com.serinus.loto.controllers.ScraperController.scrapCuponazo
GET        /scraper/once/historic                com.serinus.loto.controllers.ScraperController.scrapHistoricCuponazo
GET        /scraper/euromillones                 com.serinus.loto.controllers.ScraperController.scrapEuromillones
GET        /scraper/euromillones/historic        com.serinus.loto.controllers.ScraperController.scrapHistoricEuromillones
GET        /scraper/primitiva                    com.serinus.loto.controllers.ScraperController.scrapPrimitiva
GET        /scraper/primitiva/historic           com.serinus.loto.controllers.ScraperController.scrapHistoricPrimitiva
GET        /scraper/bonoloto                     com.serinus.loto.controllers.ScraperController.scrapBonoloto
GET        /scraper/bonoloto/historic            com.serinus.loto.controllers.ScraperController.scrapHistoricBonoloto
GET        /scraper/gordo                        com.serinus.loto.controllers.ScraperController.scrapGordo
GET        /scraper/gordo/historic               com.serinus.loto.controllers.ScraperController.scrapHistoricGordo
GET        /scraper/lototurf                     com.serinus.loto.controllers.ScraperController.scrapLototurf
GET        /scraper/lototurf/historic            com.serinus.loto.controllers.ScraperController.scrapHistoricLototurf
# Let the part begin!
GET        /scraper/all                          com.serinus.loto.controllers.ScraperController.scrapAllHistoricData

# Test
